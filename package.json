{
  "name": "jsdb",
  "version": "2.5.0",
  "description": "Simple NOSQL file-based database",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist/ node_modules/.cache/ .nyc_output/ coverage/",
    "format": "prettier-standard src/*.js test/*.js",
    "lint": "standard src/*.js test/*.js | snazzy",
    "test": "nyc ava",
    "coverage": "npm run test && nyc report --reporter=html && sirv coverage -H '0.0.0.0' -p 8000",
    "build": "rollup -c rollup.config.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ava": "^3.15.0",
    "esm": "^3.2.25",
    "npm-check-updates": "^11.3.0",
    "nyc": "^15.1.0",
    "prettier-standard": "^16.4.1",
    "rollup": "^2.44.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^1.0.11",
    "snazzy": "^9.0.0",
    "standard": "^16.0.3"
  },
  "dependencies": {
    "plock": "github:ludlovian/plock"
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
}
