{
  "name": "jsdb",
  "version": "1.1.2",
  "description": "Simple NOSQL file-based database",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist/ node_modules/.cache/ .nyc_output/ coverage/",
    "format": "prettier-standard src/*.js test/*.js",
    "lint": "standard src/*.js test/*.js | snazzy",
    "test": "nyc ava",
    "coverage": "npm run test && nyc report --reporter=html && cd coverage && python3 -m http.server 8000",
    "build": "rollup -c rollup.config.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ava": "^1.4.1",
    "esm": "^3.2.22",
    "nyc": "^14.1.0",
    "prettier-standard": "^9.1.1",
    "rollup": "^1.11.3",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-terser": "^4.0.4",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "plock": "github:ludlovian/plock"
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
}
